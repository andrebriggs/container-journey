{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kubernetes/client-node": "^0.8.2",
    "axios": "^0.19.0",
    "cli-progress": "^2.1.1",
    "cli-table": "^0.3.1",
    "commander": "^2.20.0",
    "kubernetes-client": "^6.11.1",
    "office-ui-fabric-react": "^7.10.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-http-request": "^2.0.0",
    "react-scripts-ts": "^4.0.8"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "start-cli": "nodemon --watch 'src/cli/**/*.ts' --exec 'ts-node' src/cli/cli.ts",
    "create-cli": "npm run build-cli",
    "build-cli": "tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "devDependencies": {
    "@types/cli-progress": "^1.8.1",
    "@types/cli-table": "^0.3.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "azure-storage": "^2.10.3",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "assets": [
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "src/**/*.ts",
      "./tsconfig.json"
    ]
  }
}
